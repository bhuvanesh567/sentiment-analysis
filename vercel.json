{
    "version": 2,
    "builds": [
        {
            "src": "App.py",
            "use": "@vercel/python"
        },
        {
            "src": "requirements.txt",
            "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "App.py"
        }
    ]
}
